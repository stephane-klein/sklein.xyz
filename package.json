    "scrollama": "3.2.0",
    "slug": "8.2.3",
    "svelte": "4.2.9",
    "svelte-preprocess": "5.1.3",
    "svelte-preprocess-import-assets": "1.1.0",
    "vite": "5.0.12",
    "@sveltejs/enhanced-img": "0.1.8",
    "postgres": "3.4.3"
