{
    "name": "my-app",
    "version": "0.0.1",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "package": "svelte-kit package",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "prettier-check": "prettier --check --plugin-search-dir=.",
        "lint": "prettier --check --plugin-search-dir=. . && eslint .",
        "format": "prettier --write --plugin-search-dir=. .",
        "lighthouse": "mkdir -p reports; lighthouse https://sklein.xyz/fr/ --output=html --output-path=./reports/report.html --chrome-flags='--headless'"
    },
    "devDependencies": {
        "@sveltejs/adapter-static": "2.0.2",
        "@sveltejs/kit": "1.21.0",
        "@typescript-eslint/eslint-plugin": "5.60.1",
        "@typescript-eslint/parser": "5.60.1",
        "autoprefixer": "10.4.14",
        "browserslist": "4.21.9",
        "eslint": "8.44.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-plugin-svelte3": "4.0.0",
        "postcss": "8.4.24",
        "postcss-import": "15.1.0",
        "postcss-load-config": "4.0.1",
        "postcss-nested": "6.0.1",
        "postcss-normalize": "10.0.1",
        "prettier": "2.8.8",
        "prettier-plugin-svelte": "2.10.1",
        "svelte": "4.0.1",
        "svelte-check": "3.4.4",
        "svelte-preprocess": "5.0.4",
        "svelte-preprocess-import-assets": "1.0.1",
        "tslib": "2.6.0",
        "typescript": "5.1.6",
        "vite": "4.3.9",
        "vite-imagetools": "5.0.4",
        "lighthouse": "10.3.0"
    },
    "type": "module",
    "dependencies": {
        "cheerio": "1.0.0-rc.12",
        "glob": "10.3.1",
        "gray-matter": "4.0.3",
        "js-yaml": "4.1.0",
        "markdown-it": "13.0.1",
        "@types/markdown-it": "12.2.3",
        "markdown-it-anchor": "8.6.7",
        "markdown-it-toc-done-right": "4.2.0",
        "scrollama": "3.2.0",
        "slug": "8.2.2"
    }
}
