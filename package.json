{
    "name": "my-app",
    "version": "0.0.1",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build",
        "package": "svelte-kit package",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "prettier-check": "prettier --check --plugin-search-dir=.",
        "lint": "prettier --check --plugin-search-dir=. . && eslint .",
        "format": "prettier --write --plugin-search-dir=. .",
        "lighthouse": "mkdir -p reports; lighthouse https://sklein.xyz/fr/ --output=html --output-path=./reports/report.html --chrome-flags='--headless'"
    },
    "devDependencies": {
        "@sveltejs/adapter-static": "2.0.1",
        "@sveltejs/kit": "1.11.0",
        "@typescript-eslint/eslint-plugin": "5.54.1",
        "@typescript-eslint/parser": "5.54.1",
        "autoprefixer": "10.4.13",
        "browserslist": "4.21.5",
        "eslint": "8.35.0",
        "eslint-config-prettier": "8.7.0",
        "eslint-plugin-svelte3": "4.0.0",
        "postcss": "8.4.21",
        "postcss-import": "15.1.0",
        "postcss-load-config": "4.0.1",
        "postcss-nested": "6.0.1",
        "postcss-normalize": "10.0.1",
        "prettier": "2.8.4",
        "prettier-plugin-svelte": "2.9.0",
        "svelte": "3.55.1",
        "svelte-check": "3.1.0",
        "svelte-preprocess": "5.0.1",
        "svelte-preprocess-import-assets": "0.2.6",
        "tslib": "2.5.0",
        "typescript": "4.9.5",
        "vite": "4.1.4",
        "vite-imagetools": "4.0.18",
        "lighthouse": "10.1.0"
    },
    "type": "module",
    "dependencies": {
        "cheerio": "1.0.0-rc.12",
        "glob": "9.2.1",
        "gray-matter": "4.0.3",
        "js-yaml": "4.1.0",
        "markdown-it": "13.0.1",
        "@types/markdown-it": "12.2.3",
        "markdown-it-anchor": "8.6.7",
        "markdown-it-toc-done-right": "4.2.0",
        "scrollama": "3.2.0",
        "slug": "8.2.2"
    }
}
