    "autoprefixer": "10.4.17",
    "browserslist": "4.22.2",
    "cheerio": "1.0.0-rc.12",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-svelte3": "4.0.0",
    "glob": "10.3.10",
    "gray-matter": "4.0.3",
    "js-yaml": "4.1.0",
